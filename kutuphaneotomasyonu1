-- Name: kitapkayit(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.kitapkayit() RETURNS text
    LANGUAGE plpgsql
    AS $$
DECLARE 
    kitap "kitap"%ROWTYPE;
	sonuc TEXT;
BEGIN
    sonuc := '';
	FOR kitap IN SELECT * FROM kitap LOOP
	  sonuc:=sonuc || kitap."kitapid" || E'\t' || kitap."kitapadi" || E'\r\n';
	  END LOOP;
    RETURN sonuc;
END;
$$;



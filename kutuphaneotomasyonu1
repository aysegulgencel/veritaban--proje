CREATE FUNCTION public.cevirmenekle() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
   NEW."cevirmensoyadi"=UPPER(NEW."cevirmensoyadi");
   IF NEW."cevirmendili" IS NULL THEN
      RAISE EXCEPTION 'Çevirmen dili alanı boş olamaz!';
   END IF;
   RETURN NEW;
END;
$$;



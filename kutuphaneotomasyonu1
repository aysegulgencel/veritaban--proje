CREATE FUNCTION public.basimtarihidegisikligi() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    IF NEW."tarih" <> OLD."tarih" THEN
        INSERT INTO "basimtarihidegisikligi"("tarih", "eskitarihi", "yenitarihi", "degisiklikTarihi")
        VALUES( OLD."tarih", NEW."tarih", CURRENT_TIMESTAMP::TIMESTAMP);
    END IF;

    RETURN NEW;
END;
$$;


ALTER FUNCTION public.basimtarihidegisikligi() OWNER TO postgres;

